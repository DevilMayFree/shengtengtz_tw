      GetRumdomStr = function () {
          var str = "";
          var maxNum = 10;
          var minNum = 0;
          var n = Math.floor(Math.random() * (maxNum - minNum + 1)) + minNum;
          for (var i = 1; i <= 4; i++) {
              n = Math.floor(Math.random() * (maxNum - minNum + 1)) + minNum;
              str += n.toString();
          }
          // console.log(str);
          return str;
      };

