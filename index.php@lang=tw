                $(".sub-nav").addClass("sub-up");
                $(".RWDmenu_plus02 .inside").addClass("is-up");
                $('#cd-search').addClass('is-up');
            } else {
                $(".sub-nav").removeClass("sub-up");
                $(".RWDmenu_plus02 .inside").removeClass("is-up");
                $('#cd-search').removeClass('is-up');
